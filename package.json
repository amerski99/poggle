{
  "name": "Poggle",
  "version": "1.0.0",
  "description": "A simple and fun rendition of the popular word game",
  "author": "Andrew Merski",
  "scripts": {
    "build": "./node_modules/.bin/webpack -w"
  },
  "babel": {
    "plugins": [
      "transform-react-jsx"
    ],
    "presets": [
      "es2015"
    ],
    "ignore": [
      "js/bundle.js",
      "node_modules/**/*.js"
    ]
  },
  "devDependencies": {
    "babel-core": "6.23.0",
    "babel-loader": "6.3.0",
    "babel-plugin-transform-react-jsx": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  }
}
